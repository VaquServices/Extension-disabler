   "icons": {
      "128": "icon128.png",
      "16": "icon16.png",
      "48": "icon48.png"
   },
   "name": "V Extension Manager",
   "options_page": "options.html",
   "permissions": [ "management", "tabs" ],
   "update_url": "http://clients2.google.com/service/update2/crx",
   "version": "1.0.0",
   "manifest_version": 2
}